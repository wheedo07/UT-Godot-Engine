config_version=5

[application]

config/name="Undertale: Engine"
config/description="와 샌즈"
config/version="1.0.7"
run/main_scene="uid://b72rkv2gjlqgd"
config/auto_accept_quit=false
config/features=PackedStringArray("4.5", "Forward Plus")
